include ../title/title
include ../tabs/tabs
include ./faq-item

mixin faq()
	+b.faq&attributes(attributes)
		+title({ align: 'left' }) Вопросы и ответы
		+e.tabs
			+tabs({
				tabs: [
					{text: 'Лицензия МЧС', active: true},
					{text: 'Ликвидация ООО'}
				]
			})
				+faq-item(1)
				+faq-item(2)
				+faq-item(3)
				+faq-item(4)
				+faq-item(5)
				+faq-item(6)

